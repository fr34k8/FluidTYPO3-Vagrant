deb http://ftp.hosteurope.de/mirror/ftp.debian.org/debian/ <%= @osversion %> contrib non-free main
deb-src http://ftp.hosteurope.de/mirror/ftp.debian.org/debian/ <%= @osversion %> contrib non-free main

deb http://security.debian.org/ <%= @osversion %>/updates contrib non-free main
deb-src http://security.debian.org/ <%= @osversion %>/updates contrib non-free main

deb http://ftp.hosteurope.de/mirror/ftp.debian.org/debian/ <%= @osversion %>-updates contrib non-free
deb-src http://ftp.hosteurope.de/mirror/ftp.debian.org/debian/ <%= @osversion %>-updates contrib non-free

deb http://http.debian.net/debian <%= @osversion %>-backports main contrib non-free

deb http://packages.dotdeb.org/ <%= @osversion %> all
deb-src http://packages.dotdeb.org/ <%= @osversion %> all

deb http://ftp.hosteurope.de/mirror/mariadb.org/repo/10.1/debian <%= @osversion %> main
deb-src http://ftp.hosteurope.de/mirror/mariadb.org/repo/10.1/debian <%= @osversion %> main

<% if @osversion == 'wheezy' %>
	deb http://packages.dotdeb.org wheezy-php55 all
	deb-src http://packages.dotdeb.org wheezy-php55 all
<% end %>
